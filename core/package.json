{
  "name": "redab-core",
  "version": "0.0.10",
  "description": "Core package for reactive programming",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
