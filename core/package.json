  "homepage": "https://github.com/humflelump/redab/tree/main/core",
  "devDependencies": {
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "26.0.0",
    "jest": "27.4.7",
    "prettier": "^2.6.2",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "files": [
    "lib/**/*"
